{
    "version": 2,
    "installCommand": "npm install",

    "builds": [
      {
        "src": "src/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.js",
        "methods":["GET" , "POST" , "PUT" , "DELETE"]

      }
    ]
  }